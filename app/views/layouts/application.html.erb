<!DOCTYPE html>
<html>
<head>
  <title>Issues</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <p>
  <% if user_signed_in? %>
    <%= link_to "Sign out", destroy_user_session_path,
      :method => :delete %>
  <% else %>
    <%= link_to "Sign in", new_user_session_path %>
    <%= link_to "Sign up", new_user_registration_path %>
  <% end %>
  </p>

  <h1>Issues</h1>

  <%= yield %>

</body>
</html>
